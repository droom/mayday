      .belly-photo.ih#checkpoint-nav
        .segment.type
          dl.reset.credits
            dt Photo
            dd Winnie Tzp


      .belly-aside
        aside.project(role="complementary")#checkpoint-nav
          .segment.type
              dl.reset.credits
                dt Role
                dd UX &plus; UI
